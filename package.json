    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.11.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "flow-bin": "^0.36.0",
    "mocha": "^3.2.0",
    "should": "^11.1.2",
    "ws": "^1.1.1"
